<Window x:Class="SuCri.Modul2.UI.Acad.TESTUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:SuCri.Modul2.UI.Acad"
        mc:Ignorable="d"
        Title="TESTUI" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/SuCri.Modul2.UI.Acad;component/Dictionaries/Skin01.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid Background="{DynamicResource scb_clr_01 }">
        <ComboBox x:Name="cbxTheme" Height="30" Width="120" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="20" SelectionChanged="cbxTheme_SelectionChanged"  SelectedIndex="0">
            <ComboBoxItem Content="Skin01"/>
            <ComboBoxItem Content="Skin02"/>
        </ComboBox>
    </Grid>
</Window>
