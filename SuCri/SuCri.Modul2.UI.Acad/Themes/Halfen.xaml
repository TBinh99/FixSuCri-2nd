<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <Color x:Key="BackColor1">#DEE1E6</Color>
    <Color x:Key="BackColor2">#201547</Color>

    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#D71729</Color>
    <Color x:Key="winBorderColor2">#201547</Color>
    <Color x:Key="winBorderColor3">#6894D3</Color>
    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#D71729"/>
    <SolidColorBrush x:Key="color2" Color="#201547"/>
    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="titleColor2" Color="#D5CFF5"/>
    <SolidColorBrush x:Key="titleColor3" Color="#201547"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#7C80C2"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#3C4C5C"/>
    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#2D3D4D"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#2D3D4D"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#2D3D4D"/>
